{
  "name": "website-log-insights",
  "version": "1.0.0",
  "description": "A TypeScript tool for analyzing nginx access logs",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "clean": "pnpm -r clean",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "analyze": "pnpm build && node apps/cli/dist/index.js analyze",
    "clear-logs": "pnpm build && node apps/cli/dist/index.js clear"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "typescript": "^5.3.3",
    "tsx": "^4.7.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}